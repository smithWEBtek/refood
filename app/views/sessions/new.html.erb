<h1>Log In</h1>

<%= form_tag @session do %>
  <div class="form-group">
    <%= label_tag :email_address %>
      <%= text_field_tag :email %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
      <%= text_field_tag :password %>
  </div>
  <%= submit_tag "Go!", class: "btn btn-primary" %>
<% end %>
